{
    "name" : "Dark Mode",
    "version" : "1.0",
    "manifest_version" : 3,
    "description" : "Apply night mode anytime, anywhere!",
    "background" : {
        "service_worker" : "script.js"
    },
    "permissions" : ["storage"],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : {
            "16" : "icon.png",
            "32" : "icon.png",
            "48" : "icon.png",
            "128" : "icon.png"
            
        }
    }
}